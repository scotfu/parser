<string> ::= <quoted_string>|<unquoted_string>|<host_name_string>
<conf> ::= <global_conf> <host_confs>
<global_conf> :: =  global  "{"  <key_value_pairs>  "}"  "" |";"
<host_confs> ::= <host_conf>|<host_confs><host_conf>
<host_conf> ::= host  <host_name>  "{"  <key_value_pairs> "}" 'empty'|";" 
<key_value_pairs> ::= <key_value>|<key_value_pairs>  <key_value>
<key_value> ::= <key> "="  <int>|<float>|<string> \n
<host_name> ::= <host_name_string>


Token Type
<key> = [_\-a-zA-Z][[:ascii:]]*
<int> ::= [0-9]+
<float> ::= [0-9]+\.[0-9]+ 
<quoted_string> ::= ".*"
<unquoted_string> =  [a-zA-Z_][a-zA-Z0-9\.\-_\\/]+
<host_name_string> = [a-zA-Z0-9\.\-_]+
=
}
{
;
